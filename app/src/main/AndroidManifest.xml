<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="miloszratajczyk.aplikacjanachwaleboza">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_round"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".SActivity"
                android:parentActivityName=".ListActivity">
        </activity>
        <activity
                android:name=".SetActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".SongActivity"
                android:parentActivityName=".SActivity">
        </activity>
        <activity
                android:name=".ListActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".BibleActivity"
                  android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".BooksActivity"
                  android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".SermonActivity"
                  android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>